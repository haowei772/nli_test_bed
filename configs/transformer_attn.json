{
    "MISC": {
        "print_every_n_batch": 100,
        "eval_every_n_batch": 100,
        "save_model": true,
        "save_path": "results",
        "restore_model": false,
        "restore_path": "results/best_snapshot_acc_25__iter_0_model.pt",
        "log_path": "runs"
    },
    "DATA": {
        "dataset": "snli",
        "tokenize": false,
        "max_len": 500
    },
    "TRAIN" : {
        "mode": "train",
        "seed": 42,
        "gpu": 0,
        "epochs": 50,
        "lr": 0.001,
        "batch_size": 128,
        "criterion": "cross_entropy_loss",
        "optimizer": "adam",
        "loss_compute": "simple_loss_compute",
        "eps": 1e-6
    },
    "EMBEDDING": {
        "word_vectors": "glove.6B.100d",
        "vector_cache": ".vector_cache/input_vectors.pt",
        "d_embed": 100,
        "train_embed": true,
        "positional_encoding": true,
        "apply_weight_embed": true
    },
    "ARCHITECTURE": {
        "siamese": false,
        "encoder": ["transformer"],
        "attention": ["attention_vanilla"],
        "aggregator": "linear_aggregate",
        "reducer": "mean",
        "use_residual": true,
        "layer_norm": true,
        "d_proj": 300,
        "n_layers": 2,
        "n_cells": 1,
        "d_hidden": 100,
        "d_transformer": 100,
        "d_ff_transformer": 2048,
        "n_head":10,
        "hidden_size": 100,
        "input_size": 100,
        "dp_ratio": 0.2,
        "dropout_pe": 0.2,
        "attn_pdrop": 0.1,
        "resid_pdrop": 0.1,
        "afn": "gelu"
    }
}